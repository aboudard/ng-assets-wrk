{
  "name": "@abo/ng-lib",
  "version": "0.2.0",
  "schematics": "./schematics/collection.json",
  "peerDependencies": {
    "@angular/common": "^15.2.10",
    "@angular/core": "^15.2.10"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "ng-add": {
		"save": "dependencies"
	},
  "ng-update": {
    "migrations": "./schematics/migrations.json"
  },
  "exports": {
    ".": {
      "sass": "./assets/lib-styles.scss"
    },
    "./variables": {
      "sass": "./assets/_lib-variables.scss"
    },
    "./mixins": {
      "sass": "./assets/_lib-mixins.scss"
    }
  }
}